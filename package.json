{
  "name": "weather_map",
  "version": "1.0.0",
  "description": "weather forecasting application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "node server.js",
    "client": "npm run start --prefix client",
    "install-client": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run install-client && npm run build",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diuguide/weather_map.git"
  },
  "author": "Everett Diuguid",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/diuguide/weather_map/issues"
  },
  "homepage": "https://github.com/diuguide/weather_map",
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.5.3",
    "concurrently": "^5.3.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.10.13",
    "mquery": "^3.2.3",
    "react-bootstrap": "^1.4.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "redux-devtools": "^3.7.0"
  }
